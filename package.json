{
  "name": "stack-pack-gpapi",
  "description": "A GP stack package that provides secure access to the main GP API",
  "version": "1.1.11",
  "author": "GoodPractice",
  "license": "UNLICENSED",
  "homepage": "https://github.com/gp-technical/stack-pack-gpapi",
  "repository": {
    "type": "git",
    "url": "https://github.com/gp-technical/stack-pack-gpapi"
  },
  "main": "build/index.js",
  "scripts": {
    "lint": "eslint *.js src",
    "build": "babel --presets env,stage-2 -d build/ src/",
    "prepublishOnly": "npm run build"
  },
  "swapDep": {
    "local": {
      "dep": {
        "stack-pack-request": "file:../stack-pack-request"
      },
      "dev": {}
    },
    "published": {
      "dep": {
        "stack-pack-request": "1.1.8"
      },
      "dev": {}
    }
  },
  "dependencies": {
    "stack-pack-request": "1.1.8",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.17.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^3.1.0"
  }
}